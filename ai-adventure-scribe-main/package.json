{
  "name": "infinite-realms",
  "private": true,
  "version": "0.10.0",
  "type": "module",
  "scripts": {
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry-run": "standard-version --dry-run",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:crewai\"",
    "dev:frontend": "vite",
    "predev:frontend": "bash scripts/predev-frontend.sh",
    "dev:backend": "npm run server:dev",
    "predev:backend": "bash scripts/predev-backend.sh",
    "predev:crewai": "bash scripts/predev-crewai.sh",
    "dev:crewai": "bash -c 'source .env 2>/dev/null || true && export OPENROUTER_API_KEY && crewai-service/.venv/bin/uvicorn main:app --reload --port 8000 --app-dir crewai-service --reload-exclude \"crewai-service/.venv/*\" --reload-exclude \"crewai-service/__pycache__/*\"'",
    "dev:full": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:database\"",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit && tsc -p server/tsconfig.json --noEmit",
    "type-coverage": "type-coverage -p tsconfig.app.json && type-coverage -p server/tsconfig.json",
    "preview": "vite preview",
    "server:dev": "npm run server:build && npm run server:start",
    "server:build": "tsc -p server/tsconfig.json",
    "server:start": "DOTENV_CONFIG_PATH=server/.env PORT=8888 node server/dist/server/src/index.js",
    "server:start:ci": "sh -c 'npm run server:build && node server/dist/server/src/index.js'",
    "start:prod": "NODE_ENV=production PORT=8888 node server/dist/server/src/index.js",
    "server:seed-bard-spells-supabase": "ts-node --project server/tsconfig.json server/src/scripts/seed-bard-spells-supabase.ts",
    "server:test": "vitest run --dir server",
    "test:migrations": "bash scripts/test-migrations.sh",
    "e2e": "playwright test",
    "seed:test-data": "node scripts/seed-test-data.js",
    "seed:foundry": "ts-node --esm db/seeds/foundry-example-data.ts",
    "install:all": "npm install && cd server && npm install",
    "mvp:setup": "echo 'Setting up MVP...' && npm install && echo 'Next: Copy .env.example to .env.local and configure your Supabase credentials'",
    "mvp:build": "npm run lint && npm run build",
    "check-deps": "npx depcheck || true",
    "check-unused-exports": "npx ts-prune --paths ./tsconfig.json || true",
    "check-case": "node scripts/check-case-duplicates.js",
    "check-dead": "npm run check-deps && npm run check-unused-exports && npm run check-case",
    "security-lint": "node scripts/security-lint.js",
    "security-test": "vitest run --dir server/tests/security",
    "docs:generate": "npm run server:build && node server/dist/server/scripts/generate-openapi.js",
    "docs:serve": "npm run dev"
  },
